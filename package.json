{
    "name": "@svgies/icons",
    "version": "1.1.0",
    "description": "Generate unique SVG identicons for Arweave, EVM, Solana, and Bitcoin addresses",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "bin": {
        "svgie": "dist/cli.js"
    },
    "scripts": {
        "lint": "tsc",
        "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts",
        "release": "pnpm build && changeset publish"
    },
    "keywords": [
        "arweave",
        "ethereum",
        "evm",
        "identicon",
        "svg",
        "avatar",
        "blockchain",
        "typescript"
    ],
    "author": "7i7o",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/*",
        "src/*",
        "examples/*",
        "benchmarks/*",
        ".github/",
        ".gitignore",
        "package.json",
        "tsconfig.json",
        "README.md",
        "LICENSE",
        "CHANGELOG.md"
    ],
    "homepage": "https://github.com/svgies/icons#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/svgies/icons.git"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.1",
        "@types/node": "^20.11.19",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@noble/hashes": "^2.0.1"
    }
}
