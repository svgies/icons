{
    "name": "@svgies/icons",
    "version": "2.0.1",
    "description": "Generate unique SVG identicons for Arweave, EVM, Solana, and Bitcoin addresses",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "bin": {
        "svgie": "dist/cli.mjs"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "lint": "tsc",
        "build": "tsup",
        "test": "vitest run",
        "release": "pnpm build && changeset publish"
    },
    "keywords": [
        "arweave",
        "ethereum",
        "evm",
        "identicon",
        "svg",
        "avatar",
        "blockchain",
        "typescript"
    ],
    "author": "7i7o",
    "license": "MIT",
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/*",
        "README.md",
        "LICENSE"
    ],
    "homepage": "https://github.com/svgies/icons#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/svgies/icons.git"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.1",
        "@types/node": "^20.11.19",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3",
        "vitest": "^4.0.6"
    },
    "dependencies": {
        "@noble/hashes": "^2.0.1"
    }
}
